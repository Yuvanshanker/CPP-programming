#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,count=0;
    cout<<"Enter the number: ";
    cin>>n;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            count++;
        }
    }
    if(count==2)
    {
        cout<<n<<" is a Prime number";
    }
    else{
        cout<<n<<" is not a Prime number";
    }
    return 0;
}
