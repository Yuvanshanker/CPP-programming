#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,sum=0;
    cout<<"Enter the number: ";
    cin>>n;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
           cout<<i<<endl;
           sum +=i;
        }
        
    }
    cout<<"Sum of factors of "<<n<<" is: "<<sum;
    
    return 0;
}
