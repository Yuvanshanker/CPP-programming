#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,sum=0;
    cout<<"Enter the number: ";
    cin>>n;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
           cout<<i<<endl;
           sum +=i;
        }
        
    }
    if(2*n==sum)
    {
        cout<<"It is a perfect number";
    }
    else{
        cout<<"Not a perfect number";
    }
    return 0;
}
