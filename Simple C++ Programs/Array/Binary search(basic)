#include<bits/stdc++.h>
using namespace std;

int main()
{
    int A[10],n=10;
    int key;
    cout<<"Enter n: ";
    
    for(int i=0;i<n;++i)
    {
        cin<<A[i];
    }
    cout<<"Enter key: ";
    cin<<key;
    
    for(int i=0;i<n;++i)
    {
        if(key==A[i])
        {
            cout<<"Key is found at"<< i;
            return 0;
        }
        
        
    }
    cout<<"Key is not found";
    return 0;
    
}
